{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}